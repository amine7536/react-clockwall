{
  "name": "react-clockwall",
  "version": "1.4.7",
  "description": "Digital Clocks made with ReactJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/amine7536/clockwall"
  },
  "keywords": [
    "react",
    "react-component",
    "clock",
    "world-clock",
    "timezone",
    "vite",
    "ES6",
    "vitest"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "build-lib": "babel --out-dir lib/ src/components/",
    "build-docs": "jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template --readme README.md"
  },
  "main": "lib/Clock.js",
  "author": "pixelfactory.io",
  "license": "MIT",
  "dependencies": {
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.5",
    "@babel/core": "^7.26.5",
    "@babel/preset-env": "^7.26.5",
    "@babel/preset-react": "^7.26.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^2.1.8",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "ink-docstrap": "^1.1.0",
    "jsdoc": "^4.0.4",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "postcss-nested": "^7.0.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  }
}
