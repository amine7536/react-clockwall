{
  "name": "react-clockwall",
  "version": "1.4.7",
  "description": "Digital Clocks made with ReactJS",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/amine7536/clockwall"
  },
  "keywords": [
    "react",
    "react-component",
    "clock",
    "world-clock",
    "timezone",
    "vite",
    "typescript",
    "vitest"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src test --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src test --ext .js,.jsx,.ts,.tsx --fix",
    "build-lib": "vite build --config vite.lib.config.ts",
    "build-docs": "typedoc",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "main": "lib/Clock.js",
  "types": "lib/Clock.d.ts",
  "author": "pixelfactory.io",
  "license": "MIT",
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "date-fns": "^4.1.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@eslint/js": "^9.39.1",
    "@storybook/addon-docs": "^10.0.7",
    "@storybook/addon-onboarding": "^10.0.7",
    "@storybook/react-vite": "^10.0.7",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.39.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^10.0.7",
    "ink-docstrap": "^1.1.0",
    "jsdoc": "^4.0.4",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "postcss-nested": "^7.0.2",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^2.1.8"
  }
}
