{
  "name": "react-clockwall",
  "version": "1.4.7",
  "description": "Digital Clocks made with ReactJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/amine7536/clockwall"
  },
  "keywords": [
    "react",
    "react-component",
    "clock",
    "world-clock",
    "timezone",
    "vite",
    "typescript",
    "vitest"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src test --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src test --ext .js,.jsx,.ts,.tsx --fix",
    "build-lib": "tsc --project tsconfig.lib.json && babel --out-dir lib/ src/components/ --extensions .ts,.tsx",
    "build-docs": "jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template --readme README.md",
    "typecheck": "tsc --noEmit"
  },
  "main": "lib/Clock.js",
  "types": "lib/Clock.d.ts",
  "author": "pixelfactory.io",
  "license": "MIT",
  "dependencies": {
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.5",
    "@babel/core": "^7.26.5",
    "@babel/preset-env": "^7.26.5",
    "@babel/preset-react": "^7.26.5",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.8",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "ink-docstrap": "^1.1.0",
    "jsdoc": "^4.0.4",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "postcss-nested": "^7.0.2",
    "typescript": "^5.9.3",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  }
}
